<h1>Games#new</h1>
<%= @letters %>
<form action="/score" method="post">
   <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="text" name="user_answer" placeholder="Guess a word!">
  <input type="hidden" name="letter" value="<%= @letters %>">
  <input type="hidden" name="start_time" value="<%= @start_time %>">
  <input type="submit" value="submit!">
</form>
<p>Find me in app/views/games/new.html.erb</p>

